{
  "name": "dankcommerce",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "NODE_ENV=development nest start --watch",
    "start:debug": "NODE_ENV=development nest start --debug --watch",
    "start:prod": "node dist/main",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js --config ./ormconfig.js migration:run",
    "migration:drop": "ts-node ./node_modules/typeorm/cli.js --config ./ormconfig.js schema:drop",
    "migration:refresh": "npm run migration:drop && npm run migration:run",
    "generate-library": "npm-run-all generate-library:*",
    "generate-library:csharp": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g csharp -o ./dist-client/csharp_api_client",
    "generate-library:elm": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g elm -o ./dist-client/elm_api_client",
    "generate-library:go": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g go -o ./dist-client/go_api_client",
    "generate-library:java": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g java -o ./dist-client/java_api_client",
    "generate-library:javascript": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g javascript -o ./dist-client/javascript_api_client",
    "generate-library:php": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g php -o ./dist-client/php_api_client",
    "generate-library:python": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g python -o ./dist-client/python_api_client",
    "generate-library:perl": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g perl -o ./dist-client/perl_api_client",
    "generate-library:rust": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g rust -o ./dist-client/rust_api_client",
    "generate-library:lua": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g lua -o ./dist-client/lua_api_client",
    "generate-library:r": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g r -o ./dist-client/r_api_client",
    "generate-library:ts-node": "openapi-generator-cli generate -i http://localhost:3000/explorer-json -g typescript-node -o ./dist-client/ts-node_api_client",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "NODE_ENV=test jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^7.10.0",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/elasticsearch": "^7.1.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.6",
    "@nestjs/terminus": "^7.0.1",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "config": "^3.3.3",
    "package": "^1.0.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.3",
    "swagger-ui-express": "^4.1.5",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@openapitools/openapi-generator-cli": "^2.1.12",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
